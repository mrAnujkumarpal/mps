<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://www.emoney.com/mps/api/topup"
            xmlns:tns="http://www.emoney.com/mps/api/topup"

            elementFormDefault="qualified">

    <xsd:element name="inquiryPendingTopUpRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="language" type="xsd:string" />
                <xsd:element name="trxDateTime" type="xsd:dateTime" />
                <xsd:element name="transmissionDateTime" type="xsd:dateTime" />
                <xsd:element name="companyCode" type="xsd:int" />
                <xsd:element name="channelID" type="xsd:string" />
                <xsd:element name="terminalID" type="xsd:string" />
                <xsd:element name="billKey1" type="xsd:string" />
                <xsd:element name="billKey2" type="xsd:string" />
                <xsd:element name="billKey3" type="xsd:string" />
                <xsd:element name="reference1" type="xsd:string" />
                <xsd:element name="reference2" type="xsd:string" />
                <xsd:element name="reference3" type="xsd:string" />
                <xsd:element name="reference4" type="xsd:string" />
                <xsd:element name="reference5" type="xsd:string" />
                <xsd:element name="reference6" type="xsd:string" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="inquiryResult">
        <xsd:complexType>
            <xsd:sequence>

                <xsd:element name="currency" type="xsd:string" />
                <xsd:element name="billInfo1" type="xsd:string" />
                <xsd:element name="billInfo2" type="xsd:string" />
                <xsd:element name="billInfo3" type="xsd:string" />
                <xsd:element name="billInfo4" type="xsd:string" />
                <xsd:element name="billInfo5" type="xsd:string" />
                <xsd:element name="billInfo6" type="xsd:string" />
                <xsd:element name="billInfo7" type="xsd:string" />
                <xsd:element name="billInfo8" type="xsd:string" />
                <xsd:element name="billCode" type="xsd:int" />
                <xsd:element name="billName" type="xsd:string" />
                <xsd:element name="billShortName" type="xsd:string" />
                <xsd:element name="billAmount" type="xsd:string" />

                <xsd:element name="isError" type="xsd:boolean" />
                <xsd:element name="errorCode" type="xsd:string" />
                <xsd:element name="statusDescription" type="xsd:string" />

                <xsd:element name="criteriaMismatch" type="xsd:string"
                             maxOccurs="unbounded" />

            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>


    <xsd:element name="pendingTopUpRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="language" type="xsd:string" />
                <xsd:element name="trxDateTime" type="xsd:dateTime" />
                <xsd:element name="transmissionDateTime" type="xsd:dateTime" />
                <xsd:element name="companyCode" type="xsd:int" />
                <xsd:element name="channelID" type="xsd:string" />
                <xsd:element name="terminalID" type="xsd:string" />
                <xsd:element name="billKey1" type="xsd:string" />
                <xsd:element name="billKey2" type="xsd:string" />
                <xsd:element name="billKey3" type="xsd:string" />
                <xsd:element name="paidBills" type="xsd:string" />
                <xsd:element name="paymentAmount" type="xsd:int" />
                <xsd:element name="currency" type="xsd:string" />
                <xsd:element name="transactionId" type="xsd:string" />
                <xsd:element name="reference1" type="xsd:string" />
                <xsd:element name="reference2" type="xsd:string" />
                <xsd:element name="reference3" type="xsd:string" />
                <xsd:element name="reference4" type="xsd:string" />
                <xsd:element name="reference5" type="xsd:string" />
                <xsd:element name="reference6" type="xsd:string" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="paymentResult">
        <xsd:complexType>
            <xsd:sequence>

                <xsd:element name="billInfo1" type="xsd:string" />
                <xsd:element name="billInfo2" type="xsd:string" />
                <xsd:element name="billInfo3" type="xsd:string" />
                <xsd:element name="billInfo4" type="xsd:string" />
                <xsd:element name="billInfo5" type="xsd:string" />
                <xsd:element name="billInfo6" type="xsd:string" />
                <xsd:element name="billInfo7" type="xsd:string" />
                <xsd:element name="billInfo8" type="xsd:string" />
                <xsd:element name="billInfo9" type="xsd:string" />
                <xsd:element name="billInfo10" type="xsd:string" />
                <xsd:element name="billInfo11" type="xsd:string" />

                <xsd:element name="isError" type="xsd:boolean" />
                <xsd:element name="errorCode" type="xsd:string" />
                <xsd:element name="statusDescription" type="xsd:string" />

                <xsd:element name="criteriaMismatch" type="xsd:string"
                             maxOccurs="unbounded" />

            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>


    <xsd:element name="reversePendingTopUpRequest">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="language" type="xsd:string" />
                <xsd:element name="trxDateTime" type="xsd:dateTime" />
                <xsd:element name="origTrxDateTime" type="xsd:dateTime" />
                <xsd:element name="transmissionDateTime" type="xsd:dateTime" />
                <xsd:element name="origTransmissionDateTime" type="xsd:dateTime" />
                <xsd:element name="companyCode" type="xsd:int" />
                <xsd:element name="channelID" type="xsd:string" />
                <xsd:element name="terminalID" type="xsd:string" />
                <xsd:element name="billKey1" type="xsd:string" />
                <xsd:element name="billKey2" type="xsd:string" />
                <xsd:element name="billKey3" type="xsd:string" />
                <xsd:element name="paymentAmount" type="xsd:int" />
                <xsd:element name="currency" type="xsd:string" />
                <xsd:element name="transactionId" type="xsd:string" />
                <xsd:element name="reference1" type="xsd:string" />
                <xsd:element name="reference2" type="xsd:string" />
                <xsd:element name="reference3" type="xsd:string" />
                <xsd:element name="reference4" type="xsd:string" />
                <xsd:element name="reference5" type="xsd:string" />
                <xsd:element name="reference6" type="xsd:string" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>


    <xsd:element name="reverseResult">
        <xsd:complexType>
            <xsd:sequence>

                <xsd:element name="isError" type="xsd:boolean" />
                <xsd:element name="errorCode" type="xsd:string" />
                <xsd:element name="statusDescription" type="xsd:string" />

                <xsd:element name="criteriaMismatch" type="xsd:string"
                             maxOccurs="unbounded" />

            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>


</xsd:schema>